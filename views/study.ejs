        <div id="r_requestDay" style="display:none;"><%= requestDay %></div>
        <div id="r_isquarter" style="display:none;"><%= isquarter %></div>
        <div id="r_from" style="display:none;"><%= from %></div>
        <div id="r_to" style="display:none;"><%= to %></div>
